{
  "root": true,
  "plugins": [
    "html"
  ],
  "extends": "standard",
  "rules": {
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "no-debugger": 1,
    "no-unused-vars": 1,
    "spaced-comment": 1,
    "operator-linebreak": [
      1,
      "before",
      {
        "overrides": {
          "?": "after",
          ":": "after"
        }
      }
    ]
  }
}
